<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LabCloud</display-name>
  <servlet>
    <servlet-name>LabCloudService</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
        <param-name>jersey.config.server.provider.packages</param-name>
        <param-value>edu.utdallas.hackutd.labcloud.lab</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>LabCloudService</servlet-name>
    <url-pattern>/labcloud/lab/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LabCloudClient</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
        <param-name>jersey.config.server.provider.packages</param-name>
        <param-value>edu.utdallas.hackutd.labcloud.client</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>LabCloudClient</servlet-name>
    <url-pattern>/labcloud/client/*</url-pattern>
  </servlet-mapping>
</web-app>